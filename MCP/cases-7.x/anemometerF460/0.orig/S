FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      S;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    inlet 
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet 
    {
        type            zeroGradient;
        value           uniform 0;
    }
    fixedWalls
    {
        type            zeroGradient;
    }
    movingWalls 
    {
        type            zeroGradient;
    }
    AMI1
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    AMI2
    {
        type            cyclicAMI;
        value           uniform 0;
    }
}

