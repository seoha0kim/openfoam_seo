FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        // type            zeroGradient;
        type            symmetryPlane;
    }

    bottom
    {
        // type            zeroGradient;
        type            symmetryPlane;
    }

    inlet
    {
        type            zeroGradient;
        // type            fixedValue;
        // value           uniform 101.325;
        // type            freestreamPressure;
    }

    outlet
    {
        // type            zeroGradient;
        type            fixedValue;
        value           uniform 0;
        // type            freestreamPressure;
    }

    deck
    {
        // type            zeroGradient;
        type            symmetry;
    }

    frontAndBack
    {
        type            empty;
    }

    defaultFaces
    {
        type            empty;
    }
}

