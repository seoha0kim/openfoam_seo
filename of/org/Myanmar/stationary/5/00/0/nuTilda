FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

// internalField   uniform 0.14;
internalField   uniform 0.017756;

boundaryField
{
    inlet
    {
        type            freestream;
        // freestreamValue uniform 0.14;
        freestreamValue uniform 0.017756;
    }

    outlet
    {
        type            freestream;
        // freestreamValue uniform 0.14;
        freestreamValue uniform 0.017756;
    }

    top
    {
        type            symmetryPlane;
    }

    bottom
    {
        type            symmetryPlane;
    }

    deck
    {
        type            fixedValue;
        value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }
}

