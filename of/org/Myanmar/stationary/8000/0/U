FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

// internalField   uniform (25.75 3.62 0);
internalField   uniform (8000 0 0);

boundaryField
{
    inlet
    {
        type            freestream;
        // freestreamValue uniform (25.75 3.62 0);
        freestreamValue uniform (8000 0 0);
    }

    outlet
    {
        type            freestream;
        // freestreamValue uniform (25.75 3.62 0);
        freestreamValue uniform (8000 0 0);
    }

    top
    {
        type            symmetryPlane;
    }

    bottom
    {
        type            symmetryPlane;
    }

    deck
    {
        type            noSlip;
    }

    frontAndBack
    {
        type            empty;
    }
}

